{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\""
        }
    ],
    "surroundingPairs": [
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s*(?:[A-Z][a-zA-Z0-9_]*\\s+)?function\\b.*$",
        "decreaseIndentPattern": "^\\s*$"
    }
}